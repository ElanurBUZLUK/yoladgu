# Database Configuration
DATABASE_URL=sqlite+aiosqlite:///./test.db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=yoladgu
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# Vector Database Configuration
VECTOR_DB_COLLECTION=default_collection
VECTOR_DB_SIZE=384
QDRANT_HOST=localhost
QDRANT_PORT=6333

# HNSW Configuration
HNSW_M=64
HNSW_EF_CONSTRUCTION=800
HNSW_EF_SEARCH=512
HNSW_MAX_ELEMENTS=100000

# FAISS Configuration
FAISS_NLIST=100
FAISS_NPROBE=10
FAISS_M=8
FAISS_BITS=8

# RRF Configuration
RRF_K=60

# E5 Model Configuration
E5_MODEL_NAME=intfloat/e5-large-v2
E5_DIM=1024

# ML Backend Selector Configuration
ML_SELECTOR_ENABLED=true
ML_SELECTOR_MODEL_PATH=backend/data/ml/backend_selector.joblib
ML_SELECTOR_FEATURE_ORDER_PATH=backend/data/ml/feature_order.json

# IRT Configuration
IRT_MIN_ATTEMPTS_PER_ITEM=10
IRT_MIN_ATTEMPTS_PER_STUDENT=5
IRT_CALIBRATION_ITERATIONS=5

# Retrieval Logging Configuration
RETRIEVAL_LOG_BUFFER_SIZE=100
RETRIEVAL_LOG_FLUSH_INTERVAL=30

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=1

# Security
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Development
DEBUG=false
RELOAD=false
